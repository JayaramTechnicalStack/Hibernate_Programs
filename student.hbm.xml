<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<!-- 3 -->
<class name ="com.orm.Student" table="student">

	<id name="Stno" column="student_no" type="java.lang.Integer">
	 	<generator class="increment"></generator>
	 
	</id>
		
	<property name="Sname" column="student_name">
	</property>	
	<property name="Saddr" column="student_addr"></property>
	<property name="Course"></property>
	
</class>
	
</hibernate-mapping>